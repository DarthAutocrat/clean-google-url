javascript: (() => {var turl=window.location;turl=new URL(turl);var tq=new URL(turl).searchParams;var q='',s='',n='',f='';if(tq.get("q")){q="q="+encodeURIComponent(tq.get("q"))}if(tq.get("start")){s="&start="+encodeURIComponent(tq.get("start"))}if(tq.get("num")){n="&num="+encodeURIComponent(tq.get("num"))}if(tq.get("filter")){f="&filter="+encodeURIComponent(tq.get("filter"))}prompt("Copy your cleaned Google URL" , turl.protocol+'//'+turl.host+turl.pathname+"?"+q+s+n+f);})();
